{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "framework": "vite",
  "build": {
    "env": {
      "VITE_API_URL": "https://wealth-pulse-dash-production.up.railway.app"
    }
  }
}
